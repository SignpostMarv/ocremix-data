{
	"compilerOptions": {
		"allowJs": false,
		"allowSyntheticDefaultImports": false,
		"declaration": true,
		"declarationMap": true,
		"forceConsistentCasingInFileNames": true,
		"experimentalDecorators": true,
		"lib": [
			"esnext"
		],
		"module": "ESNext",
		"newLine": "lf",
		"noEmitOnError": true,
		"noErrorTruncation": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strict": true,
		"declarationDir": "./src/",
		"rootDir": "./",
		"target": "ESNext"
	},
	"exclude": [
	]
}
